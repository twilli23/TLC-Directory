<div class="wvu-main-content">

  <div class="wrap content-wrapper">

    <div id="sidecontent" class="col-4 col-s-12 side-content-left">
         <h3>Admin Navigation</h3>
    </div>

    <div class="col-8 sol-s-12 wvu-main-content-right">
      <tbody>
        <h1>Users Website(s)</h1>
        <table>
          <thead>
            <tr>
              <th>User ID</th>
              <th>Website Url</th>
              <th colspan="3"></th>
            </tr>
          </thead>

          <tbody>
            <% @user.websites.each do |site| %>
              <tr>
                <td><%= site.user_id %></td>
                <td><%= site.website_url %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </tbody>
    </div>
  </div>
</div>
